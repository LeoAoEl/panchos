import Intro from "@components/home/Intro.astro";
import SocialFeed from "@components/home/SocialFeed.astro";
import ActionC from "@components/home/ActionC.astro";
import Banner from "@components/home/Banner.astro";
---

<Layout>
  <HeroSlider client:load />
  <Container>
    <Intro />
    <ActionC />
    <Products />
  </Container>
  <Banner />
  <Container>
    <SocialFeed />
  </Container>
</Layout>
